{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "demo-test",
  "version": "0.1.0",
  "identifier": "Demo",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Loading",
        "label": "Loading",
        "fullscreen": false,
        "resizable": false,
        "maximized": false,
        "decorations": false,
        "width": 750,
        "height": 500,
        "transparent": true,
        "center": true,
        "shadow": false,
        "minimizable": false,
        "always-on-top": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "./icons/32x32.png",
      "./icons/128x128.png",
      "./icons/128x128@2x.png",
      "./icons/icon.icns",
      "./icons/icon.ico"
    ]
  }
}
